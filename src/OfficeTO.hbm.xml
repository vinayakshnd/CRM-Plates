<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="crm.to.OfficeTO" table="agency_info">
      <meta attribute="class-description">
         This class contains the employee detail. 
      </meta>
      <id name="Office_Id" type="int" column="Office_Id">
         <generator class="identity"/>
      </id>
      <property name="Office_Name" column="Office_Name" type="string"/>
      <property name="Office_Add" column="Office_Add" type="string"/>
      <property name="Office_Phn" column="Office_Phn" type="string"/>
      
   </class>
</hibernate-mapping>